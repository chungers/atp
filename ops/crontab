# Starts the logger at 4:00 am each morning and stops at 6:00 pm
# min, hour, mday, month, wday, command
59 0 * * 1,2,3,4,5 sh /Users/david/gyokuro/lab616/cpp-ib/bin/run_logger.sh
0 18 * * 1,2,3,4,5 sh /Users/david/gyokuro/lab616/cpp-ib/bin/run_logger.sh --stop

# firehose setup
45 0 * * 1,2,3,4,5 curl -i "http://build.lab616.com:8080/job/firehose-start/buildWithParameters?token=fhstart&cause=cron"
59 0 * * 1,2,3,4,5 sh /Users/david/github/lab616/atp/bin/firehose.sh -r
30 17 * * 1,2,3,4,5 sh /Users/david/github/lab616/atp/bin/firehose.sh -c
45 17 * * 1,2,3,4,5 curl -i "http://build.lab616.com:8080/job/firehose-stop/build?token=fhstop&cause=cron"

